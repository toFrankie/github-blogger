{
  "name": "vscode-blog",
  "displayName": "vscode-blog",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Notebooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/toFrankie/vscode-blog.git"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "GitHub Issue Blog",
      "properties": {
        "vscode-blog.github.token": {
          "type": "string",
          "default": ""
        },
        "vscode-blog.github.user": {
          "type": "string",
          "default": ""
        },
        "vscode-blog.github.repo": {
          "type": "string",
          "default": ""
        }
      }
    },
    "commands": [
      {
        "command": "vscode-blog.open",
        "title": "Open vscode blog"
      },
      {
        "command": "vscode-blog.config",
        "title": "Config vscode blog"
      }
    ]
  },
  "scripts": {
    "package": "vsce package --no-dependencies",
    "prepackage": "run-s build:webview-ui build:ext",
    "build:ext": "webpack --mode production --devtool hidden-source-map",
    "start:ext": "webpack --watch",
    "start:webview-ui": "cd webview-ui && pnpm start",
    "build:webview-ui": "cd webview-ui && pnpm build",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint 'src/**/*.{js,jxs}' --cache --fix",
    "lint:prettier": "prettier . '!**/pnpm-lock.yaml' --cache --write",
    "lint:stylelint": "stylelint 'src/**/*.css' --cache --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@ifanrx/eslint-config-standard": "^1.2.6",
    "@ifanrx/eslint-config-standard-react": "^1.1.2",
    "@ifanrx/prettier-config-standard": "^1.1.2",
    "@ifanrx/stylelint-config-standard": "^1.1.3",
    "@types/vscode": "^1.83.1",
    "@vscode/vsce": "^2.21.1",
    "babel-loader": "^9.1.3",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.2",
    "@octokit/core": "^5.0.1",
    "core-js": "^3.33.1",
    "vscode-webview-rpc": "^0.0.4"
  }
}
