{
  "name": "vscode-blog",
  "displayName": "vscode-blog",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-blog.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "ext:compile": "webpack",
    "ext:watch": "webpack --watch",
    "webview-ui:start": "cd webview-ui && pnpm start",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint 'src/**/*.{js,ts,txs}' --cache --fix",
    "lint:prettier": "prettier . '!**/pnpm-lock.yaml' --cache --write",
    "lint:stylelint": "stylelint 'src/**/*.css' --cache --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@ifanrx/eslint-config-standard": "^1.2.6",
    "@ifanrx/eslint-config-standard-react": "^1.1.2",
    "@ifanrx/prettier-config-standard": "^1.1.2",
    "@ifanrx/stylelint-config-standard": "^1.1.3",
    "@types/vscode": "^1.83.1",
    "babel-loader": "^9.1.3",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.2",
    "core-js": "^3.33.1"
  }
}
